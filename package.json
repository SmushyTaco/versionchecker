{
  "name": "@smushytaco/versionchecker",
  "version": "1.0.2",
  "description": "A version checker like npm outdated but better",
  "type": "module",
  "author": {
    "name": "SmushyTaco",
    "email": "personal@nikanradan.com"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmushyTaco/versionchecker.git"
  },
  "bugs": {
    "url": "https://github.com/SmushyTaco/versionchecker/issues"
  },
  "bin": {
    "versionchecker": "dist/index.mjs"
  },
  "scripts": {
    "clean": "del-cli dist",
    "build": "npm run clean && npx tsx esbuild.ts",
    "prepare": "npm run build",
    "start": "node ./dist/index.mjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=11.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "version",
    "outdated",
    "update"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "pacote": "^21.0.0",
    "semver": "^7.6.3",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@types/pacote": "^11.1.8",
    "@types/semver": "^7.5.8",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "typescript-eslint": "^8.18.2",
    "tsx": "^4.19.2",
    "esbuild": "^0.24.2",
    "globals": "^15.14.0",
    "del-cli": "^6.0.0"
  }
}
