{
  "name": "@smushytaco/versionchecker",
  "version": "1.0.5",
  "description": "A version checker like npm outdated but better",
  "type": "module",
  "author": {
    "name": "SmushyTaco",
    "email": "personal@nikanradan.com"
  },
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SmushyTaco/versionchecker.git"
  },
  "bugs": {
    "url": "https://github.com/SmushyTaco/versionchecker/issues"
  },
  "bin": {
    "versionchecker": "dist/index.mjs"
  },
  "scripts": {
    "clean": "npx del-cli dist",
    "build": "npm run clean && npx vite build",
    "prepare": "npm run build",
    "start": "node ./dist/index.mjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=11.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "version",
    "outdated",
    "update"
  ],
  "dependencies": {
    "picocolors": "^1.1.1",
    "pacote": "^21.0.0",
    "semver": "^7.6.3",
    "table": "^6.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.10.6",
    "@types/pacote": "^11.1.8",
    "@types/semver": "^7.5.8",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "typescript-eslint": "^8.20.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "vite": "^6.0.7",
    "vite-plugin-tsc-transpile": "^1.0.4",
    "globals": "^15.14.0",
    "del-cli": "^6.0.0"
  }
}
